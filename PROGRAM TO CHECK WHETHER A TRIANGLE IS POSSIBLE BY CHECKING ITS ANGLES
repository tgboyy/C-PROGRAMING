// PROGRAM TO CHECK WHETHER A TRIANGLE IS POSSIBLE BY CHECKING ITS ANGLES.
#include <stdio.h>
int main()
{
	int n,a1,a2,a3;
	printf(" Enter angle 1  :  ");
	scanf("%d",&a1);
	printf(" Enter angle 2  :  ");
	scanf("%d",&a2);
	printf(" Enter angle 3  :  ");
	scanf("%d",&a3);
	n=a1+a2+a3;
	if(n==180)
   	printf(" Triangle is possible ");
     	else
        	printf(" Triangle is not possible ");
	return 0;
}
