// PROGRAM TO CHECK WHETHER A CHARACTER IS A VOWEL OR A CONSONENT.
#include <stdio.h>
int main()
{
	char n;
	printf(" Enter an alphabet  :  ");
	scanf("%c",&n);
    if((n=='a')||(n=='e')||(n=='i')||(n=='o')||(n=='u')||(n=='A')||(n=='E')||(n=='I')||(n=='O')||(n=='U'))
   	printf(" Alphabet is a vowel ");
     	else
       	printf(" Alphabet is a consonent ");
	return 0;
}
